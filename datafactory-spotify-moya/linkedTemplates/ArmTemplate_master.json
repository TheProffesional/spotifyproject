{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-spotify-moya"
		},
		"SpotifyDataLakeLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SpotifyDataLakeLinkedService'"
		},
		"SpotifySqlDatabaseLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SpotifySqlDatabaseLinkedService'"
		},
		"SpotifyDataLakeLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifystoragemoya.dfs.core.windows.net/"
		},
		"SpotifySqlDatabaseLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "server-spotify-project.database.windows.net"
		},
		"SpotifySqlDatabaseLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotify-sql-database"
		},
		"SpotifySqlDatabaseLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "moya-admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-spotify-moya_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SpotifyDataLakeLinkedService_accountKey": {
						"value": "[parameters('SpotifyDataLakeLinkedService_accountKey')]"
					},
					"SpotifySqlDatabaseLinkedService_password": {
						"value": "[parameters('SpotifySqlDatabaseLinkedService_password')]"
					},
					"SpotifyDataLakeLinkedService_properties_typeProperties_url": {
						"value": "[parameters('SpotifyDataLakeLinkedService_properties_typeProperties_url')]"
					},
					"SpotifySqlDatabaseLinkedService_properties_typeProperties_server": {
						"value": "[parameters('SpotifySqlDatabaseLinkedService_properties_typeProperties_server')]"
					},
					"SpotifySqlDatabaseLinkedService_properties_typeProperties_database": {
						"value": "[parameters('SpotifySqlDatabaseLinkedService_properties_typeProperties_database')]"
					},
					"SpotifySqlDatabaseLinkedService_properties_typeProperties_userName": {
						"value": "[parameters('SpotifySqlDatabaseLinkedService_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}